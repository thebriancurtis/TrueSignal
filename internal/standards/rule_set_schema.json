{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "patternProperties": {
    "^[a-z0-9_-]+$": {
      "type": "object",
      "properties": {
        "includes": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^[a-z0-9_-]+:[A-Z]{2}-R\\d{3}$|^[A-Z]{2}-R\\d{3}$|^[a-z0-9_-]+$"
          }
        },
        "excludes": {
          "type": "array",
          "items": {
            "type": "string",
            "pattern": "^[a-z0-9_-]+:[A-Z]{2}-R\\d{3}$|^[A-Z]{2}-R\\d{3}$"
          }
        }
      },
      "required": [
        "includes"
      ],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}